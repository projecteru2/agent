pid: /tmp/agent.pid # required
core: 127.0.0.1:5001 # required
heartbeat_interval: 120

auth:
  username: username
  password: password

docker:
  endpoint: unix:///var/run/docker.sock # required

metrics:
  step: 30 # required
  transfers:
    - 127.0.0.1:8125
api:
  addr: 127.0.0.1:12345

log:
  forwards:
    - tcp://127.0.0.1:5144
  stdout: false

healthcheck:
  interval: 120 # required
  timeout: 10
  cache_ttl: 300
  enable_selfmon: false

global_connection_timeout: 15s

etcd:
  machines:
    - 127.0.0.1:2379
  prefix: /agent-selfmon
